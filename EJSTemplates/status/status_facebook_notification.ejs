<% for (var i=0; i<updates.length; i++) {%> <%     var update = updates[i]; var comments = update.comments; var likes = update.likes; %> <article class="notification facebook unread" id="<%= update.id %>" data-key="<%= update.key %><%= update.id %>" data-service="facebook" data-account-key="<%= update.account.key %>" > <div class="avatarNotification"> <img src="http://graph.facebook.com/<%= update.from.id %>/picture" width="30" height="30"/> </div> <div class="update"> <p><%= update.message %></p> <% if (update.picture) { %> <a href="<%= update.link %>" rel="url" class="statusImage"><img src="<%= update.picture %>"/></a> <% } %> <% if (update.name) { %> <a href="<%= update.link %>" rel="url"><p class="updateText"><%= update.name %></p></a> <% } %> <% if (update.caption) { %> <p class="updateText"><%= update.caption %></p> <% } %> <% if (update.description) { %> <p class="updateText"><%= update.description %></p> <% } %> </div> <footer> <p> <% if(!update.isNotification) { %> <%= update.getFacebookProfileLink(update.from.id, update.from.name) %> <% } %> <% if(update.to && update.to.length) { %> &gt; <%= update.getFacebookProfileLink(update.to[0].id, update.to[0].name) %> <% } %> <a href="<%= update.getChirpURL() %>" rel="url"> <time pubdate datetime="<%= update.created.toISOString() %>" data-time="<%= update.created.getTime() %>"> <%= update.created_time %> </time> </a> </p> <% if (!update.isNotification) { %> <nav> <ul class="action2"> <li> <% if(likes){%> <a href="#" title="like" rel="like" class="button-icon button-minimal button-thin i-like show"><span></span><%= likes %></a> <% } else { %> <a href="#" title="like" rel="like" class="button-icon button-minimal button-thin i-like"><span></span>likes</a> <%}%> </li> <li> <% if(comments && comments.count) {%> <a href="#" title="comments" rel="comments" class="button-icon button-minimal button-thin i-comments show"><span></span><%= comments.count %></a> <%} else {%> <a href="#" title="comments" rel="comments" class="button-icon button-minimal button-thin i-comments iconBtn"><span></span>comments</a> <%}%> </li> </ul> </nav> <% } %> </footer> </article> <% } %>