<% for (var i=0; i<tweets.length; i++) {%> <% var tweet = tweets[i], user = tweet.getMainUser(), created = tweet.created %> <article class="twitter comment" id="<%= tweet.id %>" data-key="<%= tweet.key %><%= tweet.id %>" data-service="twitter" data-account-key="<%= tweet.account.key %>" > <div class="avatarConv"> <img src="<%= user.profileImageURL %>" width="30" height="30"/> <span class="avatarHighlight"></span> </div> <div class="update"> <p class="updateText"><%= tweet.htmlText %></p> </div> <footer> <p> <a href="http://twitter.com/<%= user.screenName %>" rel="user"><%= user.screenName %></a> <a href="<%= tweet.getChirpURL() %>" rel="url"> <time pubdate datetime="<%= created.toISOString() %>" data-time="<%= created.getTime() %>"><%= TD.util.prettyDate(created) %></time> </a> </p> </footer> </article> <% } %>