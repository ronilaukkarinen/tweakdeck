<% if(data.ownLists.length > 0) {%> <h3><%= TD.util.i18n.getMessage("users_lists", data.screenName) %></h3> <ul class="list-toplevel"> <% for (var i=0; i < data.ownLists.length; i++) {%> <%     var ownList = data.ownLists[i]; %> <li> <a href="#" rel="list" data-type="list" data-service="twitter" data-key="<%= data.account.key %>" data-meta="<%= TD.util.escape(ownList.full_name) %>" data-title="<%= ownList.user.screen_name + "/" + TD.util.escape(ownList.name) %>"> <%= TD.util.escape(ownList.name) %> <span></span> </a> </li> <% } %> </ul> <% } %> <% if(data.followLists.length > 0) {%> <h3><%= TD.util.i18n.getMessage("users_followed_lists", data.screenName) %></h3> <ul class="list-toplevel"> <% for (var i=0; i < data.followLists.length; i++) {%> <%     var followList = data.followLists[i]; %> <li> <a href="#" rel="list" data-type="list" data-service="twitter" data-key="<%= data.account.key %>" data-meta="<%= TD.util.escape(followList.full_name) %>" data-title="<%= followList.user.screen_name + "/" + TD.util.escape(followList.name) %>"> <%= TD.util.escape(followList.name) %> <em><%= TD.util.i18n.getMessage("list_author", followList.user.name) %></em></em> <span></span> </a> </li> <% } %> </ul> <% } %>